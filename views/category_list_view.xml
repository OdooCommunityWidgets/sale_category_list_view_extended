<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="sale_category_list_view_extended" model="ir.ui.view">
            <field name="model">product.public.category</field>
            <field name="inherit_id" ref="website_sale.product_public_category_tree_view"/>
            <field name="arch" type="xml">
                   <xpath expr="//view/tree[@string_id='Product Product Categories']/field[@name='complete_name']" position="after">
                      <field name="id"/>
                   </xpath>
            </field>
        </record>
    </data>
</openerp>
